{
  "name": "@ContextLogic/lerna-template-test-package",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.24.0",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "start": "webpack serve --mode development --config webpack.config.dev.js",
    "build": "rm -rf dist && webpack --mode production --progress --config webpack.config.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{tsx,ts,js}' --fix",
    "prettier": "prettier --write './**/*.{tsx,ts,js}'",
    "test:unit": "jest --testPathIgnorePatterns dist/*",
    "test:e2e:md": "nightwatch --env chromeMD",
    "test:e2e:dev": "nightwatch --env chrome",
    "publish": "npm publish --registry https://npm.infra.wish.com --verbose --scope=@ContextLogic"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/main.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/styled-components": "^5.1.18",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "chromedriver": "^96.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.27.1",
    "file-loader": "^6.2.0",
    "geckodriver": "^2.0.4",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.5",
    "mini-svg-data-uri": "^1.4.3",
    "nightwatch": "^1.7.13",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "resolutions": {
    "styled-components": "^5"
  }
